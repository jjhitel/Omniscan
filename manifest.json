{
    "manifest_version": 3,
    "name": "__MSG_appName__",
    "version": "1.0.1",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "icons": {
        "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "omnibox": {
        "keyword": "scan"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "images/icon128.png"
        }
    },
    "permissions": [
        "storage"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'none';"
    }
}